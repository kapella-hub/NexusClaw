# Server
NEXUSCLAW_SERVER_HOST=0.0.0.0
NEXUSCLAW_SERVER_PORT=8080

# Database
NEXUSCLAW_DATABASE_DSN=postgres://nexusclaw:nexusclaw@localhost:5432/nexusclaw?sslmode=disable
NEXUSCLAW_DATABASE_MAX_CONNS=25
NEXUSCLAW_DATABASE_MIN_CONNS=5

# Redis
